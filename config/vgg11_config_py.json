{
    "experiment_name": "test",
    "data": {
        "path": "./data",
        "dataset": "cifar10",
        "image_channels": 1,
        "num_classes": 10,
        "batch_size": 64,
        "num_workers": 4,
        "val_split": 0.2
    },
    "model": {
        "type": "vgg11",
        "num_blocks": [2, 2, 2, 2],
        "save_path": "./models",
        "save_model": false,
        "init_method": "nfm",
        "init_mean": 0.0,
        "init_std": 5e-2,
        "agop_path": "./agop",
        "nfm_path": "./nfm",
        "pre_trained": false
    },
    "optimizer": {
        "type": "sgd",
        "lr": 1e-3,
        "momentum": 0.9,
        "weight_decay": 5e-4,
        "nestrov": false
    },
    "training": {
        "epochs": 15,
        "criterion": "cross_entropy",
        "log_path": "./logs"
    },
    "visualize": {
        "perform": false,
        "save_path": "./reports/figures/"
    }
}